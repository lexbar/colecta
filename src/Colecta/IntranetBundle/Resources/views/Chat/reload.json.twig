{"users":{% include "ColectaUserBundle:User:users.json.twig" with {"users":users} %},"inputs":[{% for input in inputs %}{"id":{{ input.id }},"text":"{% spaceless %}{{ input.text }}{% endspaceless %}","timestamp":{{ input.date | date('U')}},"timeago":"{% spaceless %}{% include "ColectaItemBundle:Default:timeago.html.twig" with { "date": input.date } %}{% endspaceless %}","username":"{{ input.user.name }}","useravatar":"{% if input.user.avatar %}{{ path('userAvatar',{'uid':input.user.id,'width':40,'height':40}) }}{% else %}/img/unknown.gif{% endif %}"}{% endfor %}]}